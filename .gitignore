# Fichiers et dossiers de cache et de configuration spécifiques à l'IDE/éditeur
/.phpunit.cache
.phpunit.result.cache
/.fleet
/.idea
/.nova
/.vscode
/.zed
.phpactor.json

# Dépendances
/node_modules
/vendor

# Fichiers d'environnement - Très important de ne PAS versionner le .env principal
.env
.env.backup
.env.production
# Il est recommandé d'avoir un .env.example versionné

# Fichiers de build et de développement frontend
/public/build
/public/hot

# Lien symbolique de stockage public (le lien lui-même, pas le contenu de storage/app/public)
/public/storage

# Fichiers de stockage Laravel qui ne doivent pas être versionnés
/storage/*.key
/storage/app/public/*
!/storage/app/public/.gitignore
/storage/framework/cache/data/*
!/storage/framework/cache/data/.gitignore
/storage/framework/sessions/*
!/storage/framework/sessions/.gitignore
/storage/framework/testing/*
!/storage/framework/testing/.gitignore
/storage/framework/views/*.php
!/storage/framework/views/.gitignore
/storage/logs/*.log
!/storage/logs/.gitignore
/storage/pail/*
!/storage/pail/.gitignore


# Fichiers de log des gestionnaires de paquets Node.js
npm-debug.log
yarn-error.log
yarn-debug.log

# Configuration Homestead (si utilisé localement et spécifique à la machine)
Homestead.json
Homestead.yaml

# Fichier d'authentification Composer (peut contenir des tokens)
/auth.json